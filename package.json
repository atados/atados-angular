{
  "name": "atados-www",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "http://github.com/atados/www.git"
  },
  "engines": {
    "node": "~0.10.0"
  },
  "scripts": {
    "chokidar": "./node_modules/pm2/bin/pm2 start --name chokidar.atados ./node_modules/chokidar-socket-emitter/cli.js",
    "gulp": "./node_modules/gulp-cli/bin/gulp.js",
    "pretest": "./node_modules/eslint/bin/eslint.js",
    "test": "./node_modules/jest-cli/bin/jest.js",
    "start": "./node_modules/pm2/bin/pm2 startOrRestart ecosystem.json",
    "production": "./node_modules/pm2/bin/pm2 startOrRestart ecosystem.json",
    "poststart": "npm run gulp && npm run chokidar",
    "postinstall": "./node_modules/gulp/bin/gulp.js build"
  },
  "jest": {
    "scriptPreprocessor": "node_modules/systemjs-jest",
    "testPathDirs": [
      "app"
    ],
    "testDirectoryName": "test",
    "testFileExtensions": [
      "spec.js",
      "spec.jsx"
    ]
  },
  "dependencies": {
    "body-parser": "^1.15.0",
    "bootstrap-sass": "^3.3.6",
    "compression": "^1.6.1",
    "cookie-parser": "^1.4.1",
    "ejs": "^2.4.1",
    "errorhandler": "^1.4.3",
    "express": "^4.13.4",
    "http2": "^3.3.2",
    "koa": "^1.2.0",
    "koa-bodyparser": "^2.0.1",
    "koa-methodoverride": "^2.0.0",
    "lodash": "^4.6.1",
    "method-override": "^2.3.5",
    "morgan": "^1.7.0",
    "prerender": "^4.0.7",
    "prerender-node": "^2.2.1",
    "static-favicon": "^2.0.0-alpha"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "browser-sync": "^2.11.1",
    "chokidar-socket-emitter": "^0.5.0",
    "del": "^2.2.0",
    "eslint": "^2.4.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "express": "^4.13.4",
    "gulp": "^3.9.1",
    "gulp-cdnizer": "^1.1.6",
    "gulp-clean": "^0.3.2",
    "gulp-cli": "^1.2.1",
    "gulp-concat": "^2.6.0",
    "gulp-copy": "0.0.2",
    "gulp-htmlmin": "^1.3.0",
    "gulp-imagemin": "^2.4.0",
    "gulp-jspm": "^0.5.8",
    "gulp-load-plugins": "^1.2.0",
    "gulp-ng-annotate": "^2.0.0",
    "gulp-pleeease": "^2.0.2",
    "gulp-sass": "^2.2.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-svgmin": "^1.2.2",
    "gulp-uglify": "^1.5.3",
    "jest-cli": "~0.9.2",
    "jspm": "^0.16.31",
    "pm2": "^1.0.2",
    "require-dir": "^0.3.0",
    "requirejs": "~2.1.15",
    "run-sequence": "^1.1.5",
    "systemjs-jest": "~1.0.0"
  },
  "jspm": {
    "directories": {
      "baseURL": "app"
    },
    "dependencies": {
      "angular": "github:angular/bower-angular@~1.3.0",
      "angular-bootstrap": "github:angular-ui/bootstrap-bower@~0.11.2",
      "angular-cookies": "github:angular/bower-angular-cookies@1.3.7",
      "angular-easyfb": "npm:angular-easyfb@^1.4.1",
      "angular-i18n": "npm:angular-i18n@~1.3.2",
      "angular-resource": "github:angular/bower-angular-resource@~1.3.0",
      "angular-sanitize": "github:angular/bower-angular-sanitize@~1.3.0",
      "angular-toastr": "github:Foxandxss/angular-toastr@^1.7.0",
      "angular-ui-router": "github:angular-ui/ui-router@~0.2.11",
      "css": "github:systemjs/plugin-css@^0.1.20",
      "events": "npm:events@^1.1.0",
      "html": "npm:systemjs-plugin-html@^0.0.7",
      "jquery-param": "npm:jquery-param@^0.2.0",
      "js-marker-clusterer": "npm:js-marker-clusterer@^1.0.0",
      "jsx": "npm:jspm-loader-jsx@^0.0.7",
      "lodash": "npm:lodash@^4.6.1",
      "ng-redux": "npm:ng-redux@^3.3.3",
      "ngreact": "github:ngReact/ngReact@^0.2.0",
      "react": "npm:react@^0.14.8",
      "react-dom": "npm:react-dom@^0.14.8",
      "react-redux": "npm:react-redux@^4.4.1",
      "redux": "npm:redux@^3.3.1",
      "redux-devtools": "npm:redux-devtools@^3.2.0",
      "restangular": "github:mgonto/restangular@~1.4.0",
      "scss": "github:theefer/plugin-sass@master",
      "text": "github:systemjs/plugin-text@^0.0.7"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4",
      "systemjs-hot-reloader": "github:capaj/systemjs-hot-reloader@^0.5.6"
    }
  },
  "eslintConfig": {
    "extends": "airbnb",
    "rules": {
      "no-multi-spaces": 2,
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "max-len": [
        2,
        150
      ],
      "camelcase": 0,
      "arrow-parens": 2,
      "space-before-function-paren": [
        2,
        "always"
      ],
      "comma-dangle": [
        2,
        "never"
      ],
      "semi": [
        2,
        "always"
      ],
      "one-var": 0,
      "consistent-this": [
        2,
        "self"
      ],
      "curly": [
        2,
        "all"
      ]
    }
  }
}
