{
  "name": "atados-client",
  "version": "3.0.0",
  "license": "GPL-3.0+",
  "repository": {
    "type": "git",
    "url": "http://github.com/atados/www.git"
  },
  "engines": {
    "node": "~0.10.0"
  },
  "scripts": {
    "chokidar": "pm2 start --name chokidar.atados chokidar-socket-emitter",
    "gulp": "gulp",
    "lint": "eslint .",
    "test": "jest",
    "start": "pm2 startOrRestart ecosystem.json",
    "server": "pm2 startOrRestart ecosystem.json",
    "poststart": "npm run gulp && npm run chokidar",
    "_postinstall": "gulp build",
    "validate": "npm ls"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "jest": {
    "bail": true,
    "watchman": true,
    "collectCoverage": true,
    "testDirectoryName": "test",
    "testPathDirs": [
      "src/components"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/src",
      "<rootDir>/node_modules/chai",
      "<rootDir>/node_modules/enzyme",
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-addons-test-utils"
    ],
    "testFileExtensions": [
      "spec.js"
    ]
  },
  "dependencies": {
    "body-parser": "^1.15.0",
    "bootstrap-sass": "^3.3.6",
    "chokidar-socket-emitter": "^0.5.0",
    "compression": "^1.6.1",
    "cookie-parser": "^1.4.1",
    "css-modules-require-hook": "^4.0.0",
    "ect": "^0.5.9",
    "ejs": "^2.4.1",
    "errorhandler": "^1.4.3",
    "express": "^4.13.4",
    "jspm": "^0.16.33",
    "method-override": "^2.3.5",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-look": "^1.0.0-beta8",
    "react-redux": "^4.4.5",
    "react-router-component": "^0.32.0",
    "redux": "^3.4.0",
    "serve-favicon": "^2.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-eslint": "^6.0.3",
    "babel-jest": "^11.0.2",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "chai": "^3.5.0",
    "del": "^2.2.0",
    "enzyme": "^2.2.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-import": "^1.5.0",
    "eslint-plugin-jsx-a11y": "^1.0.2",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^5.0.1",
    "eslint-plugin-standard": "^1.3.2",
    "gulp": "^3.9.1",
    "gulp-cli": "^1.2.1",
    "gulp-jspm": "^0.5.8",
    "gulp-load-plugins": "^1.2.0",
    "gulp-ng-annotate": "^2.0.0",
    "gulp-pleeease": "^2.0.2",
    "gulp-sass": "^2.2.0",
    "gulp-sourcemaps": "^1.6.0",
    "jest-cli": "^11.0.2",
    "jspm": "^0.16.33",
    "jss-cli": "^3.0.0",
    "pm2": "^1.0.2",
    "precommit-hook": "^3.0.0",
    "react-addons-test-utils": "^15.0.1",
    "require-dir": "^0.3.0",
    "requirejs": "~2.1.15",
    "run-sequence": "^1.1.5",
    "systemjs-jest": "^1.0.0"
  },
  "jspm": {
    "main": "app.js",
    "directories": {
      "baseURL": "src",
      "lib": "src/components"
    },
    "dependencies": {
      "css-reset-and-normalize": "npm:css-reset-and-normalize@^0.3.0",
      "html": "npm:systemjs-plugin-html@^0.0.7",
      "jquery-param": "npm:jquery-param@^0.2.0",
      "js-marker-clusterer": "npm:js-marker-clusterer@^1.0.0",
      "lodash": "npm:lodash@^4.6.1",
      "react": "npm:react@^15.0.0",
      "react-dom": "npm:react-dom@^15.0.0",
      "react-look": "npm:react-look@^1.0.0-beta8",
      "react-redux": "npm:react-redux@^4.4.5",
      "react-router-component": "npm:react-router-component@^0.32.0",
      "redux": "npm:redux@^3.3.1",
      "redux-devtools": "npm:redux-devtools@^3.2.0"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-plugin-add-module-exports": "npm:babel-plugin-add-module-exports@^0.1.2",
      "babel-plugin-transform-decorators-legacy": "npm:babel-plugin-transform-decorators-legacy@^1.3.4",
      "babel-plugin-transform-react-jsx": "npm:babel-plugin-transform-react-jsx@^6.7.5",
      "babel-preset-react": "npm:babel-preset-react@^6.5.0",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4",
      "net": "github:jspm/nodelibs-net@^0.2.0-alpha",
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.9",
      "systemjs-hot-reloader": "github:capaj/systemjs-hot-reloader@^0.5.7",
      "tty": "github:jspm/nodelibs-tty@^0.2.0-alpha"
    },
    "overrides": {
      "npm:babel-runtime@5.8.38": {
        "main": false,
        "dependencies": {},
        "optionalDependencies": {
          "core-js": "^1.2.0"
        }
      },
      "npm:browserify-zlib@0.1.4": {
        "dependencies": {
          "readable-stream": "^2.0.2",
          "pako": "~0.2.0"
        },
        "map": {
          "_stream_transform": "readable-stream/transform"
        }
      },
      "npm:esprima@2.7.2": {
        "jspmNodeConversion": false
      },
      "npm:inherits@2.0.1": {
        "ignore": [
          "test.js",
          "node_modules",
          "node_modules",
          "node_modules",
          "node_modules",
          "node_modules",
          "node_modules",
          "node_modules",
          "node_modules",
          "node_modules",
          "node_modules",
          "node_modules",
          "node_modules",
          "node_modules",
          "node_modules",
          "node_modules",
          "node_modules",
          "node_modules",
          "node_modules",
          "node_modules",
          "node_modules",
          "node_modules",
          "node_modules"
        ]
      },
      "npm:readable-stream@1.0.34": {
        "map": {
          "stream": "stream-browserify/index",
          "util": "@empty"
        },
        "systemjs": {
          "main": "readable.js"
        },
        "dependencies": {
          "core-util-is": "~1.0.0",
          "isarray": "0.0.1",
          "string_decoder": "~0.10.0",
          "inherits": "~2.0.1",
          "stream-browserify": "~1.0.0"
        }
      },
      "npm:whatwg-fetch@0.11.0": {
        "jspmNodeConversion": false
      }
    }
  },
  "eslintConfig": {
    "extends": "airbnb",
    "rules": {
      "no-multi-spaces": 2,
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "max-len": [
        2,
        150
      ],
      "camelcase": 0,
      "arrow-parens": 2,
      "space-before-function-paren": [
        2,
        "always"
      ],
      "comma-dangle": [
        2,
        "never"
      ],
      "semi": [
        2,
        "always"
      ],
      "one-var": 0,
      "consistent-this": [
        2,
        "self"
      ],
      "curly": [
        2,
        "all"
      ]
    }
  }
}
