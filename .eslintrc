{
  "root": true,
  "parser": "babel-eslint",
  "globals": {
    "angular": true,
    "console": true,
    "after": true,
    "before": true,
    "beforeEach": true,
    "describe": true,
    "it": true
  },
  "extends": "airbnb",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "comma-dangle": 0,
    "no-console": 0,
    "no-var": 0,
    "space-before-function-paren": [2, {
        "anonymous": "always",
        "named": "always"
    }],
    "no-multi-str": 0,
    "quotes": [2, "single"],
    "linebreak-style": [2, "unix"],
    "no-underscore-dangle": [2, {
      "allow": [
        "__reload",
        "__unload",
      ]
    }]
  }
}
